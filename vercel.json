{
  "version": 2,
  "functions": {
    "api/webhook.py": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/cron.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "*/5 * * * *"
    }
  ],
  "installCommand": "pip install -r requirements.txt"
}
